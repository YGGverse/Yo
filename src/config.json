{
    "manticore":
    {
        "server":
        {
            "host":"127.0.0.1",
            "port":9308
        },
        "index":
        {
            "document":"yo_document"
        }
    },
    "cli":
    {
        "document":
        {
            "crawl":
            {
                "queue":
                {
                    "limit":1
                },
                "selector":
                {
                    "a:not([rel=nofollow])":
                    {
                        "attribute":"href",
                        "external":false,
                        "regex":"/.*/ui"
                    },
                    "image":
                    {
                        "attribute":"src",
                        "external":true,
                        "regex":"/.*/ui"
                    },
                    "audio":
                    {
                        "attribute":"src",
                        "external":false,
                        "regex":"/.*/ui"
                    },
                    "video":
                    {
                        "attribute":"src",
                        "external":false,
                        "regex":"/.*/ui"
                    },
                    "script":
                    {
                        "attribute":"href",
                        "external":false,
                        "regex":"/.*/ui"
                    }
                }
            },
            "snap":
            {
                "mime":
                [
                    "text/html",
                    "image/webp"
                ]
            }
        }
    }
}